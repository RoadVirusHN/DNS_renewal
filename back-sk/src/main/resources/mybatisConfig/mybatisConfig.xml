<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.web.curation.dto.UserDto" alias="UserDto" />
		<!-- <typeAlias type="com.web.curation.dto.FollowersDto" alias="FollowersDto" /> -->
		<typeAlias type="com.web.curation.dto.FollowerDto" alias="FollowersDto" />
		<typeAlias type="com.web.curation.dto.BoardDto" alias="BoardDto" />
		<typeAlias type="com.web.curation.dto.LikesDto" alias = "LikesDto"/>
		<typeAlias type="com.web.curation.dto.CommentDto" alias = "CommentDto"/>
		<typeAlias type="com.web.curation.dto.FollowingsDto" alias = "FollowingDto"/>
		<typeAlias type="com.web.curation.dto.FollowerDto" alias = "FollowerDto"/>
		<typeAlias type="com.web.curation.dto.HashtagDto" alias= "HashtagDto"/>
		<typeAlias type="com.web.curation.dto.BoardhashDto" alias= "BoardhashDto"/>
		<typeAlias type="com.web.curation.dto.ScrapeDto" alias= "ScrapeDto"/>
