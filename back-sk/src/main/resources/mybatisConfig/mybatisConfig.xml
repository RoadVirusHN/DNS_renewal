		<typeAlias type="com.web.curation.dto.NoticeDto" alias= "NoticeDto"/>
	</typeAliases>

	<mappers>
		<mapper resource="mybatisMapper/userSQLMapper.xml" />
		<!-- <mapper resource="mybatisMapper/followerSQLMapper.xml" /> -->
		<mapper resource="mybatisMapper/boardSQLMapper.xml" />
		<mapper resource="mybatisMapper/likeSQLMapper.xml" />
		<mapper resource="mybatisMapper/commentSQLMapper.xml" />
		<mapper resource="mybatisMapper/FollowingSQLMapper.xml" />
		<mapper resource="mybatisMapper/followerSQLMapper.xml" />
		<mapper resource="mybatisMapper/hashtagSQLMapper.xml"/>
		<mapper resource="mybatisMapper/boardhashSQLMapper.xml"/>
		<mapper resource="mybatisMapper/scrapeSQLMapper.xml"/>
		<mapper resource="mybatisMapper/dataSQLMapper.xml"/>
		<mapper resource="mybatisMapper/noticeSQLMapper.xml"/>
	</mappers>
